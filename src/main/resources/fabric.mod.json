{
  "schemaVersion": 1,
  "id": "dimensions",
  "version": "${version}",
  "name": "dimensions",
  "description": "Dynamic dimensions",
  "authors": [
    "thepiguy_"
  ],
  "contact": {
    "homepage": "https://github.com/RocKing1001/dynamic-dimension-generator",
    "sources": "https://github.com/RocKing1001/dynamic-dimension-generator"
  },
  "license": "MIT",
  "icon": "assets/dimensions/icon.png",
